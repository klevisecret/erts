<script type="text/javascript">
	var fpos = 1;
	var fsize = 10;
	var ftime = 3000;
	var finterv;
	function f_move(){
		if (fpos == fsize){
			$("#inside").css({top:"0px"});
			fpos = 1;
		}
		else{
			$("#inside").css({top:"-" + ((fpos*20)+1) + "px"});
			fpos++;
		}
		$("#flash").css({width:"1px"}).animate({width:"400px"}, 300);
	}
	function getAnketa(){
		if ($("#vAnk").val() > 0)
		{	$.ajax({
				type: 'POST',
				url: 'http://www.albeu.com/ajax/get_anketa.php',
				data: ({alt: $('#vAnk').val(), ank: $('.kerkesa').attr("id")}),
				success: function(html) {
					$(".sondazh").html(html);
				}
			});
		}
		else{
			alert("Zgjidhni njeren nga alternativat!")
		}
	}
	$(document).ready(function(){
		$(document).click(function(e){
			var relX = e.pageX;
			var relY = e.pageY;
			var rexPos = $("#reklame_lajm").offset();
			if((relX > rexPos.left && (relX < rexPos.left + 300)) && (relY > rexPos.top && relY < (rexPos.top + 250))){
				window.location = 'http://www.albeu.com/shqiperi/berisha-ne-cerrik-shokut-te-autobusit-i-ka-mbetur-ora-tek-eterit-e-ti/113281/';
			}
		});
		if($("#reklame_lajm").attr("vis") == "1"){
			$("#reklame_lajm").css({top:"330px", "margin-left":"100px", position:"absolute", opacity:0.01, "z-index":"999"});
			var tmp = setTimeout(function(){$('#reklame_lajm').remove();}, 10000);
		}
		
		$("img.lazy").lazyload({ 
			effect : "fadeIn"
		});
		finterv = setInterval("f_move()", ftime);
		
		$("#flash").mouseover(function(){
			clearInterval(finterv);
		});
		$("#flash").mouseleave(function(){
			finterv = setInterval("f_move();", ftime);
		});
		$(".content").corner("5px");
		$(".content_top").corner("bottom right 5px");
		$(".bar").corner("right 5px");
		$(".bar_top > div").corner("top 5px");
		$(".login").corner("top 5px");
		$(".login").css("max-width", $(".login").css("width"));
		$("#creds").corner("bottom left 5px");
		
		$(".alternativa").click(function(){
			$("#vAnk").val($(this).val());
		});
		
		$("#fraza").focusin(function(){
			if ($(this).val() == 'Kerko')
				$(this).val('');
		});
		$("#fraza").focusout(function(){
			if ($(this).val() == '')
				$(this).val('Kerko');
		});
		
		$("#k_login").click(function(){
			$("#mod_login").overlay().load();
		});
		$("#mod_login").overlay({
			top: 230,
			mask: {
				color: '#111',
				loadSpeed: 200,
				opacity: 0.5
			},
			closeOnClick: false
		});
		
				
		$(".login").mouseenter(function(){
			$("#creds").show();
			//alert($("#creds").css("display"));
		});
		$(".login").mouseleave(function(){
			$("#creds").hide();
		});
		$(window).scroll(function(){
			var wTop = $(window).scrollTop();
			//var mOffs = $("#menu_container").offset();

			if (wTop < 130){
				$("#menu_container").css({'position':'relative', 'top': '0px', 'left': '0px'});
				$("#menu").css({'position':'relative', 'top': '0'});
			}
			else{
				$("#menu_container").css({'position':'fixed', 'top': '0px', 'left': '0px'});
				$("#menu").css({'position':'relative', 'top': '0'});
			}
		});
	});
</script>
